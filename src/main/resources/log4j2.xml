<?xml version="1.0" encoding="UTF-8"?>
<configuration status="INFO">
	<Appenders>
		<Console name="stout" target="SYSTEM_OUT">
			<PatternLayout pattern="[%d{HH:mm:ss:SSS}] [%p] - %l - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<root level="info" includeLocation="true">
			<AppenderRef ref="stout" />
		</root>
	</Loggers>
</configuration>
